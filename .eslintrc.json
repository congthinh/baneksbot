{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["plugin:flowtype/recommended", "standard"],
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "parserOptions": {
    "ecmaVersion": 2016
  },
  "rules": {
    "semi": [
      "off"
    ],
    "no-console": [
      "off"
    ]
  }
}
